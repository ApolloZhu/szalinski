blade_thickness=1;
margin_thickness=3;
blade_height=12;
margin_height=2;
form="all";//[all,pig,rocket,beetle,duck,gingerbread]

if(form=="all") {
   cutter(){pig();};
   translate([40,0,0])
   cutter(){rocket();};
   translate([0,40,0])
   cutter(){beetle();};
   translate([-10,-40,0])
   cutter(){duck();};
   translate([-50,10,0])
   cutter(){gingerbread();};
} else {
    cutter() {
        if(form=="pig") {
            pig();
        } else if(form=="rocket") {
            rocket();
        } else if(form=="beetle") {
            beetle();
        } else if(form=="duck") {
            duck();
        } else if(form=="gingerbread") {
            gingerbread();
        }         
    }
}



module cutter() {
    linear_extrude(height=blade_height)
    shell1(blade_thickness) {
        children();
    };
    linear_extrude(height=margin_height)
    shell1(margin_thickness) {
        children();
    }
}

module shell1(offset) {
    difference() {
        offset(offset)
        children();
        offset(0)
        children();

    }
}



