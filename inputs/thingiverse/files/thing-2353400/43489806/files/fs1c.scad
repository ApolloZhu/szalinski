points = 3;

// Bearing diameter (mm)
diameter = 22;

// Bearing height (mm)
height = 7;

// Satellite bearing distance (mm) from center
distance = 25;

// Satellite bearing brim thickness (mm)
brim_thickness = 1.0;

/* [Thumb Initials] */
// Recessed thumb initials (blank or 1 or 2 letters)
thumb_initials = "DW";

