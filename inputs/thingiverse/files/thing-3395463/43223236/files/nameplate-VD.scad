module writetext(textstr1, textstr2, sizeit1, sizeit2,length_extrude)
{
    halignvalue = "center";

    linear_extrude(height=extrude_text, center = false)
    {
        fullfont1=str(fontname1,":style=",fontstyle1);
        fullfont2=str(fontname2_final,":style=",fontstyle2_final);
        
        translate([0,-distance_to_border,0])
            text(textstr1,size=sizeit1,font=fullfont1,halign=halignvalue,valign="top");
        translate([0,-distance_to_border-realtextsize1-distance_two_lines,0])
            text(textstr2,size=sizeit2,font=fullfont2,halign=halignvalue,valign="top"); 
    }
}



